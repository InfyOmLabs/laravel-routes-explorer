{
    "name": "infyomlabs/routes-explorer",
    "description": "Laravel Routes Explorer",
    "keywords": [
        "laravel",
        "infyom",
        "api",
        "explorer",
        "routes"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mitul Golakiya",
            "email": "me@mitul.me"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\RoutesExplorer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\RoutesExplorer\\RoutesExplorerServiceProvider"
            ]
        }
    }
}
